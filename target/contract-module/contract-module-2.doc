        int posBit = (pos) % 8;
        byte setter = (byte) (1 << (posBit));
        byte toBeSet = data[posByte];
        byte result;
        if (val == 1) {
            result = (byte) (toBeSet | setter);
        } else {
            result = (byte) (toBeSet & ~setter);

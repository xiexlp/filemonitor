        Long level = block.getNumber();
        List<BlockInfo> blockInfos = index.get(level.intValue());
        for (BlockInfo blockInfo : blockInfos) {
            if (areEqual(blockInfo.getHash(), hash)) {
                return blockInfo.totalDifficulty;
            }
        }

        return ZERO;
    }



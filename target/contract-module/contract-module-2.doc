            return (String) decoded;
        } else if (decoded instanceof byte[]) {
            return Hex.toHexString((byte[]) decoded);
        } else if (decoded instanceof Object[]) {
            String result = "";
            for (Object item : (Object[]) decoded) {
                result += asString(item);
